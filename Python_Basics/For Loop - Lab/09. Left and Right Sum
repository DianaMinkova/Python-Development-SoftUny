number = int(input())
sum_first_couple = 0
sum_second_couple = 0

for couple in range(1, number + 1):
    value = int(input())
    sum_first_couple += value

for couple in range(1, number +1):
    value = int(input())
    sum_second_couple += value

if sum_first_couple == sum_second_couple:
    print(f'Yes, sum = {abs(sum_second_couple)}')
else:
    print(f'No, diff = {abs(sum_first_couple - sum_second_couple)}')
