word = input()
letters = ''
sum_of_letters = 0

for letters in word:
    if letters == 'a':
        sum_of_letters += 1
    elif letters == 'e':
        sum_of_letters += 2
    elif letters == 'i':
        sum_of_letters += 3
    elif letters == 'o':
        sum_of_letters += 4
    elif letters == 'u':
        sum_of_letters += 5
print(sum_of_letters)
