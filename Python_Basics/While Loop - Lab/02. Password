""" First solution: """
user = input()
pass= input()
text_password = input()

while text_password != pass:
    text_password = input()

print(f'Welcome {user}!')

""" Second solution: """
username = input()
password = input()
validate_password = input()

while not validate_password == password:
    validate_password = input()

print(f'Welcome {username}!')
    
