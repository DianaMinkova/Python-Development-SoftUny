first_number = int(input())
second_number = int(input())
symbol = input()
result = 0

if symbol == '+':
    result = first_number + second_number
    if result % 2 == 0:
        print(f'{first_number} {symbol} {second_number} = {result} - even')
    elif result % 2 != 0:
        print(f'{first_number} {symbol} {second_number} = {result} - odd')
elif symbol == '-':
    result = first_number - second_number
    if result % 2 == 0:
        print(f'{first_number} {symbol} {second_number} = {result} - even')
    elif result % 2 != 0:
        print(f'{first_number} {symbol} {second_number} = {result} - odd')
elif symbol == '*':
    result = first_number * second_number
    if result % 2 == 0:
        print(f'{first_number} {symbol} {second_number} = {result} - even')
    elif result % 2 != 0:
        print(f'{first_number} {symbol} {second_number} = {result} - odd')
elif second_number == 0:
    print(f'Cannot divide {first_number} by zero')
elif symbol == '/':
    result = first_number / second_number
    print(f'{first_number} / {second_number} = {result:.2f}')
elif symbol == '%':
    result = first_number % second_number
    print(f'{first_number} % {second_number} = {result}')
    
